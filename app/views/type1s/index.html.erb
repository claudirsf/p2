<p id="notice"><%= notice %></p>

<h1>Type1s</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type1s.each do |type1| %>
      <tr>
        <td><%= type1.type %></td>
        <td><%= link_to 'Show', type1 %></td>
        <td><%= link_to 'Edit', edit_type1_path(type1) %></td>
        <td><%= link_to 'Destroy', type1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type1', new_type1_path %>
